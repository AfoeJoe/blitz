{
  "name": "blitz-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "seed": "blitz db seed",
    "push": "blitz prisma db push",
    "db-reset": "blitz prisma migrate reset",
    "db-dev": "blitz prisma migrate dev"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed2": "node --loader ts-node/esm db/seed.ts",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} db/seed.ts",
    "seed3": "node --require esbuild-register prisma/seed.ts"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "3.13.0",
    "autoprefixer": "10.x",
    "blitz": "0.45.4",
    "classnames": "2.3.1",
    "final-form": "4.20.7",
    "postcss": "8.x",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-final-form": "6.5.9",
    "tailwindcss": "3.x",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.1",
    "esbuild-register": "3.3.2",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.6.2",
    "prettier-plugin-prisma": "3.13.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "prisma": "3.13.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "private": true
}
